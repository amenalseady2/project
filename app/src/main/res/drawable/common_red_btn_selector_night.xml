<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_selected="true">
        <shape>
            <solid android:color="#88EB4E35" />

            <corners
                android:radius="60px" /> </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#88EB4E35" />

            <corners
                android:radius="60px"/> </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#935656"
                />
            <stroke
                android:width="1dp"
                android:color="#252525"></stroke>
            <corners
                android:radius="60px"
                 />
        </shape>
    </item>

</selector>